-- import the file
BULK INSERT Town1.dbo.Sales
FROM 'C:\Users\..\Sales.csv'
WITH
(
        FORMAT='CSV',
        FIRSTROW=2
)
GO

BULK INSERT Town1.dbo.Customer
FROM 'C:\Users\..\Customer.csv'
WITH
(
        FORMAT='CSV',
        FIRSTROW=2
)
GO

BULK INSERT Town1.dbo.Product
FROM 'C:\Users\..\Product.csv'
WITH
(
        FORMAT='CSV',
        FIRSTROW=2
)
GO
